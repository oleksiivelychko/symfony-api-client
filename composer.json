{
    "name": "oleksiivelychko/symfony-api-client",
    "description": "CLI client based on Symfony console skeleton which interacts with remote API.",
    "homepage": "https://github.com/oleksiivelychko/symfony-api-client",
    "type": "project",
    "license": "MIT",
    "keywords": ["console", "symfony", "api", "command"],
    "authors": [
        {
            "name": "Oleksii Velychko",
            "email": "oleksiivelychko@icloud.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/console": "^6.1.1",
        "guzzlehttp/guzzle": "^7.4.5",
        "symfony/dotenv": "^6.1.1"
    },
    "require-dev": {
        "symfony/framework-bundle": "^6.1.1",
        "symfony/test-pack": "^1.0.10",
        "phpunit/phpunit": "^9.5.21"
    },
    "autoload": {
        "psr-4": {
            "Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
